<?xml version="1.0" encoding="utf-8"?>
<data-extraction-rules>
    <cloud-backup>
        <!-- Exclude sensitive camera data from backup -->
        <exclude domain="file" path="video/" />
        <exclude domain="sharedpref" path="UserRemark" />
    </cloud-backup>
    <device-transfer>
        <exclude domain="file" path="video/" />
    </device-transfer>
</data-extraction-rules>